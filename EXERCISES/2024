        "import numpy as np\n",
        "\n",
        "# 1. Question\n",
        "A = np.array([[1, 2], [3, 4]])\n",
        "B = np.array([[5, 6], [7, 8]])\n",
        "C = np.array([[-1, 2], [3, 0]])\n",
        "D = np.array([[-1, 2, 3], [4, 0, 6]])\n",
        "E = np.array([[1, 2], [4, 5], [7, 8]])\n",
        "\n",
        "print(A + B)\n",
        "print(B - A)\n",
        "print(A + C)\n",
        "print(D + E.T)\n",
        "\n",
        "print(0.5 * A)\n",
        "print(2 * B)\n",
        "print(-3 * C)\n",
        "print(4 * D)\n",
        "\n",
        "print(A @ B)\n",
        "print(B @ A)\n",
        "print(A @ D[:, :2])\n",
        "print(D @ E)"
