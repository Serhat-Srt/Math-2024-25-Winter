        "# 21. Derivatives\n",
        "import sympy as sp\n",
        "\n",
        "x = sp.symbols('x')\n",
        "\n",
        "functions = [\n",
        "    3 * x + 3,\n",
        "    sp.sin(x),\n",
        "    4 + sp.sin(x),\n",
        "    2 * x**3 - 3 * x**2 + 8 * x - 9,\n",
        "    x**(1/3),\n",
        "    sp.exp(x),\n",
        "    sp.cos(x) + sp.sin(x),\n",
        "    2 * sp.sin(3 * x) * sp.cos(x),\n",
        "    sp.sin(x),\n",
        "    (x + 1) * sp.ln(x + 1),\n",
        "    sp.sin(x) / x,\n",
        "    sp.exp(-2 * x),\n",
        "    x / sp.sqrt(x**2 + 1)\n",
        "]\n",
        "\n",
        "for f in functions:\n",
        "    print(f\"f(x) = {f}, f'(x) = {sp.diff(f, x)}\")\n",
        "\n",
        "f = sp.ln(sp.sin(x))\n",
        "derivative = sp.diff(f, x)\n",
        "print(f\"Derivative of ln(sin(x)) = {derivative}\")\n",
        "\n",
        "f = sp.cos(x)\n",
        "f_derivative = sp.diff(f, x)\n",
        "print(f\"f'(x) = {f_derivative}, -f(x) = {-f}\")\n",
        "\n",
        "# First limit\n",
        "expr1 = sp.sin(x) / x\n",
        "limit1 = sp.limit(expr1, x, 0)\n",
        "print(f\"Limit 1: {limit1}\")\n",
        "\n",
        "# Second limit\n",
        "expr2 = sp.ln(x) / x\n",
        "limit2 = sp.limit(expr2, x, sp.oo)\n",
        "print(f\"Limit 2: {limit2}\")\n",
        "\n",
        "t = sp.symbols('t')\n",
        "position = 3 * t**2 - 6 * t + 1\n",
